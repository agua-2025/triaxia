{
  "version": 2,
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/stripe/webhook",
      "destination": "/api/stripe/webhook"
    },
    {
      "source": "/api/stripe/checkout",
      "destination": "/api/stripe/checkout"
    }
  ]
}